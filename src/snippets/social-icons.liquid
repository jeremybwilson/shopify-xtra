{% if settings.facebook_url != blank %}
  <li>
    <a href="{{ settings.facebook_url }}" target="_blank" aria-label="Facebook">
      {% include 'svg-icon-facebook' %}
    </a>
  </li>
{% endif %}

{% if settings.instagram_id != blank %}
    <li>
      <a href="//instagram.com/{{ settings.instagram_id }}" target="_blank" aria-label="Instagram">
        {% include 'svg-icon-instagram' %}
      </a>
    </li>
{% endif %}

{% if settings.youtube_url != blank %}
  <li>
    <a href="{{ settings.youtube_url }}" target="_blank" aria-label="Youtube">
      {% include 'svg-icon-youtube' %}
    </a>
  </li>
{% endif %}

{% if settings.twittername != blank %}
  <li>
    <a href="{{ settings.twittername }}" target="_blank" aria-label="Twitter">
      <i class="fa fa-twitter fa-2x"></i>
    </a>
  </li>
{% endif %}

{% if settings.pinterest_url != blank %}
  <li>
    <a href="{{ settings.pinterest_url }}" target="_blank" aria-label="Pinterest">
      <i class="fa fa-pinterest fa-2x"></i>
    </a>
  </li>
{% endif %}

{% if settings.tumblr_url != blank %}
  <li>
    <a href="{{ settings.tumblr_url }}" target="_blank" aria-label="Tumblr">
      <i class="fa fa-tumblr fa-2x"></i>
    </a>
  </li>
{% endif %}

{% if settings.googleplus_url != blank %}
  <li>
    <a href="{{ settings.googleplus_url }}" target="_blank" aria-label="GooglePlus">
      <i class="fa fa-google-plus fa-2x"></i>
    </a>
  </li>
{% endif %}

